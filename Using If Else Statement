print("Enter a number")
n = int(input())

def factorial(n):
  if n < 0:
    print("Factorial does not exist")
    factor = 0
    return factor
   elif n == 0:
    factor = 1
    return factor
    print(factor)
   else:
    factor = 1
    for i in range(1, n+1):
      factor = factor * i
    return factor
    
print("The factorial number of the number is" , factorial(n))
